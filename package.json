{
  "name": "numano-project",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "craco start",
    "start-dev-server": "craco start",
    "build": "craco build",
    "test": "node ./bin/sp-rest-proxy.js test-interactive",
    "test-debug": "node ./bin/sp-rest-proxy.js test-non-interactive",
    "test-interactive": "craco test",
    "test-non-interactive": "cross-env CI=true craco --inspect test --no-cache --runInBand",
    "test-coverage": "node ./bin/sp-rest-proxy.js test-ci",
    "test-vscode": "craco test --ci --coverage --reporters=default",
    "test-ci": "craco test --ci --coverage --reporters=default --reporters=jest-junit && node ./bin/inlineCoverageReportCSS.js",
    "analyze": "source-map-explorer build/static/js/main*",
    "analyze-vendor": "source-map-explorer build/static/js/1*",
    "eject": "craco eject",
    "prettier": "prettier --write \"src/**/*.{ts,tsx,js,jsx}\"",
    "pre-commit-prettier": "pretty-quick --staged",
    "eslint": "eslint \"src/**/*.{ts,tsx,js,jsx}\"",
    "deploy": "env-cmd -f .env.staging run-s build deploy-build",
    "deploy:production": "run-s build deploy-build-production",
    "deploy:dev1": "env-cmd -f .env.development run-s build deploy-build-dev1",
    "deploy:dev2": "env-cmd -f .env.dev run-s build deploy-build-dev2",
    "deploy-build": "ts-node --project ./bin/tsconfig.json ./bin/uploadFolderToSP.ts \"./build/**\" \"/Apps/Staging/Cafeteria\" \"staging\"",
    "deploy-build-dev1": "ts-node --project ./bin/tsconfig.json ./bin/uploadFolderToSP.ts \"./build/**\" \"/Apps/Dev1\" \"staging\"",
    "deploy-build-dev2": "ts-node --project ./bin/tsconfig.json ./bin/uploadFolderToSP.ts \"./build/**\" \"/Apps/Dev2\" \"staging\"",
    "deploy-build-production": "ts-node --project ./bin/tsconfig.json ./bin/uploadFolderToSP.ts \"./build/**\" \"/Apps/Numano\" \"production\"",
    "deploy-api": "ts-node --project ./bin/tsconfig.json ./bin/uploadFolderToSP.ts \"./src/api/**\" \"/Apps/Cafeteria\""
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "dependencies": {
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.7.4",
    "@baristalabs/craco-raw-loader": "^1.2.0",
    "@craco/craco": "^5.6.2",
    "@jackwilsdon/craco-use-babelrc": "^1.0.0",
    "@material-ui/core": "^4.10.0",
    "@material-ui/icons": "^4.5.1",
    "@material-ui/lab": "^4.0.0-alpha.56",
    "@navikt/craco-plugins": "^1.0.1",
    "@pnp/common": "^1.3.5",
    "@pnp/logging": "^1.3.5",
    "@pnp/odata": "^1.3.5",
    "@pnp/sp": "^1.3.5",
    "@sentry/browser": "^5.15.4",
    "@types/debug": "^4.1.5",
    "@types/dropzone": "^5.5.4",
    "@types/jest": "^25.2.3",
    "@types/moment-jalaali": "^0.7.4",
    "@types/node": "^13.13.9",
    "@types/react": "^16.9.49",
    "@types/react-count-to": "^0.12.0",
    "@types/react-dom": "^16.9.8",
    "@types/react-router-dom": "^5.1.3",
    "@types/react-router-hash-link": "^1.2.1",
    "@types/react-select": "^3.0.13",
    "@types/react-slick": "^0.23.4",
    "@types/sharepoint": "^2016.1.10",
    "@typescript-eslint/eslint-plugin": "^2.34.0",
    "@typescript-eslint/parser": "^2.34.0",
    "axios": "^0.19.2",
    "babel-polyfill": "^6.26.0",
    "balloon-css": "^1.1.0",
    "cache": "^2.1.0",
    "chart.js": "^2.9.3",
    "clean": "^4.0.2",
    "craco": "^0.0.3",
    "craco-babel-loader": "^0.1.4",
    "craco-less": "^1.16.0",
    "debug": "^4.1.1",
    "env-cmd": "^10.1.0",
    "eslint": "^6.8.0",
    "install": "^0.13.0",
    "lodash": "^4.17.15",
    "moment-jalaali": "^0.9.2",
    "react": "^16.13.0",
    "react-app-polyfill": "^1.0.6",
    "react-chartjs-2": "2.9.0",
    "react-context-devtool": "^1.0.1",
    "react-count-to": "^0.12.0",
    "react-dom": "^16.13.0",
