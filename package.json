    "babel-plugin-transform-remove-console": "^6.9.4",
    "envify": "^4.1.0",
    "eslint-plugin-prettier": "^3.1.1",
    "fs-jetpack": "^2.4.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.19.1",
    "react-localize-redux": "^3.5.3",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-terser": "^5.2.0",
    "sp-pnp-node": "^2.2.0",
    "terser": "^4.7.0",
    "terser-brunch": "^4.0.0",
    "ts-node": "^8.10.1",
    "uglifyify": "^5.0.2"
  }
}
